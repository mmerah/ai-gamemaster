{
  "name": "ai-gamemaster-frontend",
  "version": "1.0.0",
  "description": "Vue.js frontend for AI Game Master",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "generate-types": "cd .. && python scripts/dev/generate_ts.py",
